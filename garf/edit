#!/bin/sh

type() {
    xdotool type --clearmodifiers "$@"
}
keys() {
    xdotool key --clearmodifiers "$@"
}

nohup thunar ~/pics > /dev/null 2>&1 &
    sleep 0.5
type eo
    keys Return
type posts
    keys Return End
    keys Ctrl+t BackSpace
type unedited
    keys Return Home

i3-msg 'layout tabbed' > /dev/null

